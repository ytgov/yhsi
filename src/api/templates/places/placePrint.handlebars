<html>
	<head>
		<style>
			html, body, div, span, object, iframe, h1, h2, h3, h4, h5, h6, p,
			blockquote, pre, abbr, address, cite, code, del, dfn, em, img, ins, kbd,
			q, samp, small, strong, sub, sup, var, b, i, dl, dt, dd, ol, ul, li,
			fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr,
			th, td, article, aside, canvas, details, figcaption, figure, footer,
			header, hgroup, menu, nav, section, summary, time, mark, audio, video {
			margin: 0; padding: 0; border: 0; outline: 0; font-size: 100%;
			vertical-align: baseline; background: transparent; } @page { size: letter
			landscape; margin: 30px 0; } body { padding: 0 30px; line-height: 1;
			font-family: Arial, Helvetica, sans-serif; font-size: 14px; } .infoTable {
			border-collapse: collapse; width: 100% } h1 {font-size: 22px} .infoTable
			td { padding-top: 2px; width: 50% } .infoTable tr td:first-child
			{padding-right: 15px} .box {border: 1px #ccc solid; padding: 3px;
			border-radius: 3px; margin-top: 3px; min-height: 16px; margin-bottom: 6px;
			} .box span { font-weight: 700; display: inline-block; min-width: 100px}
			.section {margin-bottom: 25px; page-break-inside: avoid;}

		</style>
	</head>

	<body>
		<table
			style='width: 100%; margin-bottom: 15px'
			border='0'
			cellspacing='0'
			cellpadding='0'
		>
			<tr style='height: 90px'>
				<td style='width: 180px'>
					<img
						src='http://localhost:{{API_PORT}}/yukon.svg'
						style='height: 80px; width: 160px'
					/>
				</td>
				<td
					style='text-align: right; vertical-align: bottom; padding-top: 25px'
				>
					<h4 style='font-size: 1.4rem'>Yukon Historic Sites</h4>
					<p style='font-size: 1.0rem'>{{primaryName}}</p>
				</td>
			</tr>
		</table>

		<div class='section'>
			<h1>Summary</h1>
			<hr />
			<table class='infoTable' border='0' cellspacing='0' cellpadding='0'>
				<tr>
					<td>
						<strong>YHSI ID</strong><div class='box'>{{yHSIId}}</div>
						<strong>Designations</strong><div class='box'>{{designations}}</div>
						<strong>CRHP Category</strong><div class='box'>{{yHSIId}}</div>
						<strong>Site Categories</strong><div
							class='box'
							style='margin-bottom: 6px'
						>{{joinArray siteCategories}}</div>
						<strong>Records</strong><div class='box'>{{joinArray records}}</div>
						<strong>Show In Register?</strong><div
							class='box'
						>{{showInRegister}}</div>
					</td>
					<td>
						<strong>Primary Name</strong><div class='box'>{{primaryName}}</div>
						<strong>Secondary Names</strong><div class='box'>{{joinArrayPick
								names
								'description'
							}}</div>
						<strong>Contributing Resources</strong><div class='box'>{{joinArray
								contributingResources
							}}</div>
						<strong>Historical Patterns</strong>
						<div class='box'>
							{{#each historicalPatterns}}
								-
								<strong>{{historicalPatternTypeName}}</strong><br
								/>{{comments}}<br />
							{{/each}}
						</div>
					</td>
				</tr>
			</table>
		</div>

		<div class='section'>
			<h1>Location</h1>
			<hr />
			<table class='infoTable' border='0' cellspacing='0' cellpadding='0'>
				<tr>
					<td>
						<strong>Community</strong><div class='box'>{{communityName}}</div>
						<strong>Other Locality</strong><div
							class='box'
						>{{otherLocality}}</div>

						<strong>Physical Address</strong><div
							class='box'
						>{{physicalAddress}}
							<br /><span>Province:</span>{{physicalProvince}}
							<br /><span>Country:</span>{{physicalCountry}}
							<br /><span>Postal Code:</span>{{physicalPostalCode}}
						</div>

						<strong>Previous Address</strong><div
							class='box'
						>{{previousAddress}}</div>

						<strong>Latitude</strong><div class='box'>{{latitude}}</div>
						<strong>Longitude</strong><div class='box'>{{longitude}}</div>
						<strong>Coordinate Determination</strong><div
							class='box'
						>{{coordinateDeterminationName}}</div>

					</td>
					<td>
						<strong>Other Community</strong><div
							class='box'
						>{{otherCommunity}}</div>
						<strong>Context</strong><div class='box'>{{locationContext}}</div>

						<strong>NTS Map Sheet</strong><div class='box'>{{nTSMapSheet}}</div>
						<strong>Area (m2)</strong><div class='box'>{{hectareArea}}</div>
						<strong>Borden Number</strong><div
							class='box'
						>{{bordenNumber}}</div>
						<strong>Misc. Info</strong><div
							class='box'
						>{{locationComment}}</div>

					</td>
				</tr>
			</table>
		</div>

		<div class='section'>
			<h1>Dates & Condition</h1>
			<hr />
			<table class='infoTable' border='0' cellspacing='0' cellpadding='0'>
				<tr>
					<td>
					</td>
				</tr>
			</table>
		</div>

		<div class='section'>
			<h1>Themes & Function</h1>
			<hr />
			<table class='infoTable' border='0' cellspacing='0' cellpadding='0'>
				<tr>
					<td>
					</td>
				</tr>
			</table>
		</div>

		<div class='section'>
			<h1>Associations</h1>
			<hr />
			<table class='infoTable' border='0' cellspacing='0' cellpadding='0'>
				<tr>
					<td>
					</td>
				</tr>
			</table>
		</div>

		<div class='section'>
			<h1>Legal & Zoning</h1>
			<hr />
			<table class='infoTable' border='0' cellspacing='0' cellpadding='0'>
				<tr>
					<td>
					</td>
				</tr>
			</table>
		</div>

		<div class='section'>
			<h1>Photos</h1>
			<hr />
			<table class='infoTable' border='0' cellspacing='0' cellpadding='0'>
				<tr>
					<td>
					</td>
				</tr>
			</table>
		</div>

		<div class='section'>
			<h1>Management</h1>
			<hr />
			<table class='infoTable' border='0' cellspacing='0' cellpadding='0'>
				<tr>
					<td>
					</td>
				</tr>
			</table>
		</div>

		<div class='section'>
			<h1>Description</h1>
			<hr />
			<table class='infoTable' border='0' cellspacing='0' cellpadding='0'>
				<tr>
					<td>
					</td>
				</tr>
			</table>
		</div>

	</body>
</html>