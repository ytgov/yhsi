{
	"name": "yhsi-api",
	"version": "1.0.0",
	"description": "API for YHSI Application",
	"main": "server.js",
	"scripts": {
		"start": "ts-node-dev -r tsconfig-paths/register index.ts",
		"development": "pm2 start ecosystem.config.js --no-daemon",
		"test": "mocha --config ./.mocharc.js 'tests/**/*.test.ts'",
		"start-yhsif": "nodemon index.ts",
		"build": "tsc && tsc-alias",
		"knex": "ts-node -r tsconfig-paths/register ./node_modules/.bin/knex --knexfile ./config.d/knexfile.ts"
	},
	"author": "Michael Johnson",
	"license": "ISC",
	"dependencies": {
		"@sentry/node": "^7.53.1",
		"@types/axios": "^0.14.0",
		"cors": "^2.8.5",
		"dotenv": "^8.6.0",
		"express": "^4.17.1",
		"express-handlebars": "^7.1.2",
		"express-openid-connect": "^2.7.2",
		"express-session": "^1.17.1",
		"express-validator": "^6.12.1",
		"helmet": "^5.0.2",
		"json2csv": "^5.0.7",
		"knex": "^1.0.4",
		"lodash": "^4.17.21",
		"moment": "^2.29.4",
		"mssql": "^8.0.2",
		"multer": "^1.4.3",
		"pug": "^3.0.2",
		"puppeteer": "^19.8.0",
		"sharp": "0.30.5"
